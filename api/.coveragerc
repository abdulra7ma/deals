[run]
omit =
    *migrations*
    *tests*
    /usr/*
    */env/*
    */urls.py
    deals/fixtures/*
    deals/apps/accounts/api/authentication.py
    deals/apps/common/management/commands/generate_secretkey.py
    deals/apps/common/management/commands/startapp.py
    deals/apps/common/utils/redis.py
    deals/loggers.py
    deals/wsgi.py
    */admin.py

[report]
exclude_lines =
    noqa
    pragma: no cover
    def __unicode__
    def __repr__
    def __str__
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    assert

show_missing = True
